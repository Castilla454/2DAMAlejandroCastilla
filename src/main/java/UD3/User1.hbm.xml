<?xml version="1.0"?>
<hibernate-mapping>

    <class name="UD3.User" table="USER">
        <id name="id" type="int">
            <column name="id" />
            <generator class="assigned" />
        </id>
        <property name="userName" type="java.lang.String">
            <column name="USERNAME" />
        </property>
        <property name="userMessage" type="java.lang.String">
            <column name="USERMESSAGE" />
        </property>
    </class>




</hibernate-mapping>